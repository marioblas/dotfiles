format = """\
  $username\
  $hostname\
  $directory\
  $git_branch\
  $git_commit\
  $git_state\
  $git_status\
  $package\
  $nodejs\
  $cmd_duration\
  $line_break\
  $character\
"""

[username]
style_root = "red"
style_user = "242"

[hostname]
style = "242"

[directory]
style = "blue"

[git_branch]
symbol = ""
style = "242"

[git_commit]
style = "green"

[git_state]
style = "yellow"

[git_status]
style = "purple"
modified = "*"
deleted = "×"
stashed = "≡"
format = "([$all_status$ahead_behind]($style)) "

[package]
symbol = ""
style = "cyan"
display_private = true

[node]
style = "green"

[cmd_duration]
style = "yellow"
format = "[$duration]($style) "

[character]
success_symbol = "[❯](bold green)"
error_symbol = "[❯](bold red)"
